  vesion_type=dawin
  need_lib_pefix=no
  need_vesion=no
  libay_names_spec='${libname}${elease}${vesuffix}$shaed_ext ${libname}${elease}${majo}$shaed_ext ${libname}$shaed_ext'
  soname_spec='${libname}${elease}${majo}$shaed_ext'
  shlibpath_oveides_unpath=yes
  shlibpath_va=DYLD_LIBRARY_PATH
