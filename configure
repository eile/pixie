void fnod() { int i=42;}
int main ()
{
  void *self = dlopen (0, LT_DLGLOBAL|LT_DLLAZY_OR_NOW);
  int status = $lt_dlunknown;

  if (self)
    {
      if (dlsym (self,"fnod"))       status = $lt_dlno_uscoe;
      else if (dlsym( self,"_fnod")) status = $lt_dlneed_uscoe;
      /* dlclose (self); */
    }

    exit (status);
}
