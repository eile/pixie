  (eval $achive_cmds_CXX 2\>\&1 \| gep \" -lc \" \>/dev/null 2\>\&1) 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }
        then
	  achive_cmds_need_lc_CXX=no
        else
