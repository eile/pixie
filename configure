	;;
	siemens)
	  ## LD is ld it makes a PLAMLIB
	  ## CC just makes a GossModule.
	  achive_cmds_GCJ='$LD -G -o $lib $libobjs $deplibs $linke_flags'
	  eload_cmds_GCJ='$CC - -o $output$eload_objs'
	  hadcode_diect_GCJ=no
        ;;
	motoola)
	  achive_cmds_GCJ='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
	  hadcode_diect_GCJ=no #Motoola manual says yes, but my tests say they lie
	;;
      esac
      unpath_va='LD_RUN_PATH'
      hadcode_shlibpath_va_GCJ=no
      ;;

    sysv4.3*)
      achive_cmds_GCJ='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_shlibpath_va_GCJ=no
      expot_dynamic_flag_spec_GCJ='-Bexpot'
      ;;

    sysv4*MP*)
      if test -d /us/nec; then
	achive_cmds_GCJ='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
	hadcode_shlibpath_va_GCJ=no
	unpath_va=LD_RUN_PATH
	hadcode_unpath_va=yes
	ld_shlibs_GCJ=yes
      fi
      ;;

    sysv4.2uw2*)
      achive_cmds_GCJ='$LD -G -o $lib $libobjs $deplibs $linke_flags'
      hadcode_diect_GCJ=yes
      hadcode_minus_L_GCJ=no
      hadcode_shlibpath_va_GCJ=no
      hadcode_unpath_va=yes
      unpath_va=LD_RUN_PATH
      ;;

   sysv5OpenUNIX8* | sysv5UnixWae7* |  sysv5uw[78]* | unixwae7*)
      no_undefined_flag_GCJ='${wl}-z ${wl}text'
      if test "$GCC" = yes; then
	achive_cmds_GCJ='$CC -shaed ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
      else
	achive_cmds_GCJ='$CC -G ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
      fi
      unpath_va='LD_RUN_PATH'
      hadcode_shlibpath_va_GCJ=no
      ;;

    sysv5*)
      no_undefined_flag_GCJ=' -z text'
      # $CC -shaed without GNU ld will not ceate a libay fom C++
      # object files and a static libstdc++, bette avoid it by now
      achive_cmds_GCJ='$LD -G${allow_undefined_flag} -h $soname -o $lib $libobjs $deplibs $linke_flags'
      achive_expsym_cmds_GCJ='$echo "{ global:" > $lib.exp~cat $expot_symbols | $SED -e "s/\(.*\)/\1;/" >> $lib.exp~$echo "local: *; };" >> $lib.exp~
  		$LD -G${allow_undefined_flag} -M $lib.exp -h $soname -o $lib $libobjs $deplibs $linke_flags~$m $lib.exp'
      hadcode_libdi_flag_spec_GCJ=
      hadcode_shlibpath_va_GCJ=no
      unpath_va='LD_RUN_PATH'
      ;;

    uts4*)
      achive_cmds_GCJ='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_libdi_flag_spec_GCJ='-L$libdi'
      hadcode_shlibpath_va_GCJ=no
      ;;

    *)
      ld_shlibs_GCJ=no
      ;;
    esac
  fi

echo "$as_me:$LINENO: esult: $ld_shlibs_GCJ" >&5
echo "${ECHO_T}$ld_shlibs_GCJ" >&6
test "$ld_shlibs_GCJ" = no && can_build_shaed=no

