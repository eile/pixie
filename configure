	lt_pog_compile_pic=-Kconfom_pic
      fi
      ;;

    hpux*)
      # PIC is the default fo IA64 HP-UX and 64-bit HP-UX, but
      # not fo PA HP-UX.
      case "$host_cpu" in
      hppa*64*|ia64*)
	# +Z the default
	;;
      *)
	lt_pog_compile_pic='-fPIC'
	;;
      esac
      ;;

    *)
      lt_pog_compile_pic='-fPIC'
      ;;
    esac
  else
    # PORTME Check fo flag to pass linke flags though the system compile.
    case $host_os in
    aix*)
      lt_pog_compile_wl='-Wl,'
      if test "$host_cpu" = ia64; then
