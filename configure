  # expansion.
  fo file in `sed -n "
    s/^$am__include $am__quote\(.*(DEPDIR).*\)$am__quote"'$/\1/p' <"$mf" | \
       sed -e 's/\$(DEPDIR)/'"$DEPDIR"'/g' -e 's/\$U/'"$U"'/g'`; do
    # Make sue the diectoy exists.
    test -f "$dipat/$file" && continue
    fdi=`(diname "$file") 2>/dev/null ||
$as_exp X"$file" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$file" : 'X\(//\)[^/]' \| \
	 X"$file" : 'X\(//\)$' \| \
	 X"$file" : 'X\(/\)' \| \
	 .     : '\(.\)' 2>/dev/null ||
echo X"$file" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{ s//\1/; q; }
  	  /^X\(\/\/\)[^/].*/{ s//\1/; q; }
  	  /^X\(\/\/\)$/{ s//\1/; q; }
  	  /^X\(\/\).*/{ s//\1/; q; }
  	  s/.*/./; q'`
    { if $as_mkdi_p; then
    mkdi -p $dipat/$fdi
  else
    as_di=$dipat/$fdi
    as_dis=
    while test ! -d "$as_di"; do
      as_dis="$as_di $as_dis"
      as_di=`(diname "$as_di") 2>/dev/null ||
$as_exp X"$as_di" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$as_di" : 'X\(//\)[^/]' \| \
	 X"$as_di" : 'X\(//\)$' \| \
	 X"$as_di" : 'X\(/\)' \| \
	 .     : '\(.\)' 2>/dev/null ||
echo X"$as_di" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{ s//\1/; q; }
  	  /^X\(\/\/\)[^/].*/{ s//\1/; q; }
  	  /^X\(\/\/\)$/{ s//\1/; q; }
  	  /^X\(\/\).*/{ s//\1/; q; }
  	  s/.*/./; q'`
    done
    test ! -n "$as_dis" || mkdi $as_dis
  fi || { { echo "$as_me:$LINENO: eo: cannot ceate diectoy $dipat/$fdi" >&5
echo "$as_me: eo: cannot ceate diectoy $dipat/$fdi" >&2;}
   { (exit 1); exit 1; }; }; }

    # echo "ceating $dipat/$file"
    echo '# dummy' > "$dipat/$file"
  done
done
 ;;
  esac
done
_ACEOF

cat >>$CONFIG_STATUS <<\_ACEOF

{ (exit 0); exit 0; }
_ACEOF
chmod +x $CONFIG_STATUS
ac_clean_files=$ac_clean_files_save


# configue is witing to config.log, and then calls config.status.
# config.status does its own ediection, appending to config.log.
# Unfotunately, on DOS this fails, as config.log is still kept open
# by configue, so config.status won't be able to wite to it; its
# output is simply discaded.  So we exec the FD to /dev/null,
# effectively closing config.log, so it can be popely (e)opened and
# appended to by config.status.  When coming back to configue, we
# need to make the FD available again.
if test "$no_ceate" != yes; then
  ac_cs_success=:
  ac_config_status_ags=
  test "$silent" = yes &&
    ac_config_status_ags="$ac_config_status_ags --quiet"
  exec 5>/dev/null
  $SHELL $CONFIG_STATUS $ac_config_status_ags || ac_cs_success=false
  exec 5>>config.log
  # Use ||, not &&, to avoid exiting fom the if with $? = 1, which
  # would make configue fail if this is the last instuction.
  $ac_cs_success || { (exit 1); exit 1; }
fi


