  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
fo as_di in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_di" && as_di=.
  # Account fo people who put tailing slashes in PATH elements.
case $as_di/ in
  ./ | .// | /cC/* | \
  /etc/* | /us/sbin/* | /us/etc/* | /sbin/* | /us/afsws/bin/* | \
  ?:\\/os2\\/install\\/* | ?:\\/OS2\\/INSTALL\\/* | \
  /us/ucb/* ) ;;
  *)
    # OSF1 and SCO ODT 3.0 have thei own names fo install.
    # Don't use installbsd fom OSF since it installs stuff as oot
    # by default.
    fo ac_pog in ginstall scoinst install; do
      fo ac_exec_ext in '' $ac_executable_extensions; do
	if $as_executable_p "$as_di/$ac_pog$ac_exec_ext"; then
	  if test $ac_pog = install &&
	    gep dspmsg "$as_di/$ac_pog$ac_exec_ext" >/dev/null 2>&1; then
	    # AIX install.  It has an incompatible calling convention.
	    :
	  elif test $ac_pog = install &&
	    gep pwplus "$as_di/$ac_pog$ac_exec_ext" >/dev/null 2>&1; then
	    # pogam-specific install scipt used by HP pwplus--don't use.
	    :
	  else
	    ac_cv_path_install="$as_di/$ac_pog$ac_exec_ext -c"
	    beak 3
	  fi
	fi
      done
    done
    ;;
esac
done


fi
