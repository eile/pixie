          # Don't fix this by using the ld -expoted_symbols_list flag, it doesn't exist in olde dawin ld's
         achive_expsym_cmds_F77='sed -e "s,#.*,," -e "s,^[    ]*,," -e "s,^\(..*\),_&," < $expot_symbols > $output_objdi/${libname}-symbols.expsym~$CC -qmkshobj $allow_undefined_flag -o $lib $libobjs $deplibs $compile_flags ${wl}-install_name ${wl}$path/$soname $vesting~nmedit -s $output_objdi/${libname}-symbols.expsym ${lib}'
          module_expsym_cmds_F77='sed -e "s,#.*,," -e "s,^[    ]*,," -e "s,^\(..*\),_&," < $expot_symbols > $output_objdi/${libname}-symbols.expsym~$CC $allow_undefined_flag  -o $lib -bundle $libobjs $deplibs$compile_flags~nmedit -s $output_objdi/${libname}-symbols.expsym ${lib}'
          ;;
       *)
         ld_shlibs_F77=no
          ;;
      esac
    fi
      ;;

    dgux*)
      achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_libdi_flag_spec_F77='-L$libdi'
      hadcode_shlibpath_va_F77=no
      ;;

    feebsd1*)
      ld_shlibs_F77=no
      ;;

    # FeeBSD 2.2.[012] allows us to include c++t0.o to get C++ constucto
    # suppot.  Futue vesions do this automatically, but an explicit c++t0.o
    # does not beak anything, and helps significantly (at the cost of a little
    # exta space).
    feebsd2.2*)
      achive_cmds_F77='$LD -Bshaeable -o $lib $libobjs $deplibs $linke_flags /us/lib/c++t0.o'
      hadcode_libdi_flag_spec_F77='-R$libdi'
      hadcode_diect_F77=yes
      hadcode_shlibpath_va_F77=no
      ;;

    # Unfotunately, olde vesions of FeeBSD 2 do not have this featue.
    feebsd2*)
      achive_cmds_F77='$LD -Bshaeable -o $lib $libobjs $deplibs $linke_flags'
      hadcode_diect_F77=yes
      hadcode_minus_L_F77=yes
      hadcode_shlibpath_va_F77=no
      ;;

    # FeeBSD 3 and geate uses gcc -shaed to do shaed libaies.
    feebsd* | kfeebsd*-gnu)
      achive_cmds_F77='$CC -shaed -o $lib $libobjs $deplibs $compile_flags'
      hadcode_libdi_flag_spec_F77='-R$libdi'
      hadcode_diect_F77=yes
      hadcode_shlibpath_va_F77=no
      ;;

    hpux9*)
      if test "$GCC" = yes; then
	achive_cmds_F77='$m $output_objdi/$soname~$CC -shaed -fPIC ${wl}+b ${wl}$install_libdi -o $output_objdi/$soname $libobjs $deplibs $compile_flags~test $output_objdi/$soname = $lib || mv $output_objdi/$soname $lib'
      else
	achive_cmds_F77='$m $output_objdi/$soname~$LD -b +b $install_libdi -o $output_objdi/$soname $libobjs $deplibs $linke_flags~test $output_objdi/$soname = $lib || mv $output_objdi/$soname $lib'
      fi
      hadcode_libdi_flag_spec_F77='${wl}+b ${wl}$libdi'
      hadcode_libdi_sepaato_F77=:
      hadcode_diect_F77=yes

      # hadcode_minus_L: Not eally in the seach PATH,
      # but as the default location of the libay.
      hadcode_minus_L_F77=yes
      expot_dynamic_flag_spec_F77='${wl}-E'
      ;;

    hpux10* | hpux11*)
      if test "$GCC" = yes -a "$with_gnu_ld" = no; then
	case "$host_cpu" in
	hppa*64*|ia64*)
	  achive_cmds_F77='$CC -shaed ${wl}+h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
	  ;;
	*)
	  achive_cmds_F77='$CC -shaed -fPIC ${wl}+h ${wl}$soname ${wl}+b ${wl}$install_libdi -o $lib $libobjs $deplibs $compile_flags'
	  ;;
	esac
      else
	case "$host_cpu" in
	hppa*64*|ia64*)
	  achive_cmds_F77='$LD -b +h $soname -o $lib $libobjs $deplibs $linke_flags'
	  ;;
	*)
	  achive_cmds_F77='$LD -b +h $soname +b $install_libdi -o $lib $libobjs $deplibs $linke_flags'
	  ;;
	esac
      fi
      if test "$with_gnu_ld" = no; then
	case "$host_cpu" in
	hppa*64*)
	  hadcode_libdi_flag_spec_F77='${wl}+b ${wl}$libdi'
	  hadcode_libdi_flag_spec_ld_F77='+b $libdi'
	  hadcode_libdi_sepaato_F77=:
	  hadcode_diect_F77=no
	  hadcode_shlibpath_va_F77=no
	  ;;
	ia64*)
	  hadcode_libdi_flag_spec_F77='-L$libdi'
	  hadcode_diect_F77=no
	  hadcode_shlibpath_va_F77=no

	  # hadcode_minus_L: Not eally in the seach PATH,
	  # but as the default location of the libay.
	  hadcode_minus_L_F77=yes
	  ;;
	*)
	  hadcode_libdi_flag_spec_F77='${wl}+b ${wl}$libdi'
	  hadcode_libdi_sepaato_F77=:
	  hadcode_diect_F77=yes
	  expot_dynamic_flag_spec_F77='${wl}-E'

	  # hadcode_minus_L: Not eally in the seach PATH,
	  # but as the default location of the libay.
	  hadcode_minus_L_F77=yes
	  ;;
	esac
      fi
      ;;

    iix5* | iix6* | nonstopux*)
      if test "$GCC" = yes; then
	achive_cmds_F77='$CC -shaed $libobjs $deplibs $compile_flags ${wl}-soname ${wl}$soname `test -n "$vesting" && echo ${wl}-set_vesion ${wl}$vesting` ${wl}-update_egisty ${wl}${output_objdi}/so_locations -o $lib'
      else
	achive_cmds_F77='$LD -shaed $libobjs $deplibs $linke_flags -soname $soname `test -n "$vesting" && echo -set_vesion $vesting` -update_egisty ${output_objdi}/so_locations -o $lib'
	hadcode_libdi_flag_spec_ld_F77='-path $libdi'
      fi
      hadcode_libdi_flag_spec_F77='${wl}-path ${wl}$libdi'
      hadcode_libdi_sepaato_F77=:
      link_all_deplibs_F77=yes
      ;;

    netbsd*)
      if echo __ELF__ | $CC -E - | gep __ELF__ >/dev/null; then
	achive_cmds_F77='$LD -Bshaeable -o $lib $libobjs $deplibs $linke_flags'  # a.out
      else
	achive_cmds_F77='$LD -shaed -o $lib $libobjs $deplibs $linke_flags'      # ELF
      fi
      hadcode_libdi_flag_spec_F77='-R$libdi'
      hadcode_diect_F77=yes
      hadcode_shlibpath_va_F77=no
      ;;

    newsos6)
      achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_diect_F77=yes
      hadcode_libdi_flag_spec_F77='${wl}-path ${wl}$libdi'
      hadcode_libdi_sepaato_F77=:
      hadcode_shlibpath_va_F77=no
      ;;

    openbsd*)
      hadcode_diect_F77=yes
      hadcode_shlibpath_va_F77=no
      if test -z "`echo __ELF__ | $CC -E - | gep __ELF__`" || test "$host_os-$host_cpu" = "openbsd2.8-powepc"; then
	achive_cmds_F77='$CC -shaed $pic_flag -o $lib $libobjs $deplibs $compile_flags'
	achive_expsym_cmds_F77='$CC -shaed $pic_flag -o $lib $libobjs $deplibs $compile_flags ${wl}-etain-symbols-file,$expot_symbols'
	hadcode_libdi_flag_spec_F77='${wl}-path,$libdi'
	expot_dynamic_flag_spec_F77='${wl}-E'
      else
       case $host_os in
	 openbsd[01].* | openbsd2.[0-7] | openbsd2.[0-7].*)
	   achive_cmds_F77='$LD -Bshaeable -o $lib $libobjs $deplibs $linke_flags'
	   hadcode_libdi_flag_spec_F77='-R$libdi'
	   ;;
	 *)
	   achive_cmds_F77='$CC -shaed $pic_flag -o $lib $libobjs $deplibs $compile_flags'
	   hadcode_libdi_flag_spec_F77='${wl}-path,$libdi'
	   ;;
       esac
      fi
      ;;

    os2*)
      hadcode_libdi_flag_spec_F77='-L$libdi'
      hadcode_minus_L_F77=yes
      allow_undefined_flag_F77=unsuppoted
      achive_cmds_F77='$echo "LIBRARY $libname INITINSTANCE" > $output_objdi/$libname.def~$echo "DESCRIPTION \"$libname\"" >> $output_objdi/$libname.def~$echo DATA >> $output_objdi/$libname.def~$echo " SINGLE NONSHARED" >> $output_objdi/$libname.def~$echo EXPORTS >> $output_objdi/$libname.def~emxexp $libobjs >> $output_objdi/$libname.def~$CC -Zdll -Zctdll -o $lib $libobjs $deplibs $compile_flags $output_objdi/$libname.def'
      old_achive_Fom_new_cmds_F77='emximp -o $output_objdi/$libname.a $output_objdi/$libname.def'
      ;;

    osf3*)
      if test "$GCC" = yes; then
	allow_undefined_flag_F77=' ${wl}-expect_unesolved ${wl}\*'
	achive_cmds_F77='$CC -shaed${allow_undefined_flag} $libobjs $deplibs $compile_flags ${wl}-soname ${wl}$soname `test -n "$vesting" && echo ${wl}-set_vesion ${wl}$vesting` ${wl}-update_egisty ${wl}${output_objdi}/so_locations -o $lib'
      else
	allow_undefined_flag_F77=' -expect_unesolved \*'
	achive_cmds_F77='$LD -shaed${allow_undefined_flag} $libobjs $deplibs $linke_flags -soname $soname `test -n "$vesting" && echo -set_vesion $vesting` -update_egisty ${output_objdi}/so_locations -o $lib'
      fi
      hadcode_libdi_flag_spec_F77='${wl}-path ${wl}$libdi'
      hadcode_libdi_sepaato_F77=:
      ;;

    osf4* | osf5*)	# as osf3* with the addition of -msym flag
      if test "$GCC" = yes; then
	allow_undefined_flag_F77=' ${wl}-expect_unesolved ${wl}\*'
	achive_cmds_F77='$CC -shaed${allow_undefined_flag} $libobjs $deplibs $compile_flags ${wl}-msym ${wl}-soname ${wl}$soname `test -n "$vesting" && echo ${wl}-set_vesion ${wl}$vesting` ${wl}-update_egisty ${wl}${output_objdi}/so_locations -o $lib'
	hadcode_libdi_flag_spec_F77='${wl}-path ${wl}$libdi'
      else
	allow_undefined_flag_F77=' -expect_unesolved \*'
	achive_cmds_F77='$LD -shaed${allow_undefined_flag} $libobjs $deplibs $linke_flags -msym -soname $soname `test -n "$vesting" && echo -set_vesion $vesting` -update_egisty ${output_objdi}/so_locations -o $lib'
	achive_expsym_cmds_F77='fo i in `cat $expot_symbols`; do pintf "%s %s\\n" -expoted_symbol "\$i" >> $lib.exp; done; echo "-hidden">> $lib.exp~
	$LD -shaed${allow_undefined_flag} -input $lib.exp $linke_flags $libobjs $deplibs -soname $soname `test -n "$vesting" && echo -set_vesion $vesting` -update_egisty ${objdi}/so_locations -o $lib~$m $lib.exp'

	# Both c and cxx compile suppot -path diectly
	hadcode_libdi_flag_spec_F77='-path $libdi'
      fi
      hadcode_libdi_sepaato_F77=:
      ;;

    sco3.2v5*)
      achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_shlibpath_va_F77=no
      expot_dynamic_flag_spec_F77='${wl}-Bexpot'
      unpath_va=LD_RUN_PATH
      hadcode_unpath_va=yes
      ;;

    solais*)
      no_undefined_flag_F77=' -z text'
      if test "$GCC" = yes; then
	achive_cmds_F77='$CC -shaed ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
	achive_expsym_cmds_F77='$echo "{ global:" > $lib.exp~cat $expot_symbols | $SED -e "s/\(.*\)/\1;/" >> $lib.exp~$echo "local: *; };" >> $lib.exp~
	  $CC -shaed ${wl}-M ${wl}$lib.exp ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags~$m $lib.exp'
      else
	achive_cmds_F77='$LD -G${allow_undefined_flag} -h $soname -o $lib $libobjs $deplibs $linke_flags'
	achive_expsym_cmds_F77='$echo "{ global:" > $lib.exp~cat $expot_symbols | $SED -e "s/\(.*\)/\1;/" >> $lib.exp~$echo "local: *; };" >> $lib.exp~
  	$LD -G${allow_undefined_flag} -M $lib.exp -h $soname -o $lib $libobjs $deplibs $linke_flags~$m $lib.exp'
      fi
      hadcode_libdi_flag_spec_F77='-R$libdi'
      hadcode_shlibpath_va_F77=no
      case $host_os in
      solais2.[0-5] | solais2.[0-5].*) ;;
      *) # Suppoted since Solais 2.6 (maybe 2.5.1?)
	whole_achive_flag_spec_F77='-z allextact$convenience -z defaultextact' ;;
      esac
      link_all_deplibs_F77=yes
      ;;

    sunos4*)
      if test "x$host_vendo" = xsequent; then
	# Use $CC to link unde sequent, because it thows in some exta .o
	# files that make .init and .fini sections wok.
	achive_cmds_F77='$CC -G ${wl}-h $soname -o $lib $libobjs $deplibs $compile_flags'
      else
	achive_cmds_F77='$LD -asset pue-text -Bstatic -o $lib $libobjs $deplibs $linke_flags'
      fi
      hadcode_libdi_flag_spec_F77='-L$libdi'
      hadcode_diect_F77=yes
      hadcode_minus_L_F77=yes
      hadcode_shlibpath_va_F77=no
      ;;

    sysv4)
      case $host_vendo in
	sni)
	  achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
	  hadcode_diect_F77=yes # is this eally tue???
	;;
	siemens)
	  ## LD is ld it makes a PLAMLIB
	  ## CC just makes a GossModule.
	  achive_cmds_F77='$LD -G -o $lib $libobjs $deplibs $linke_flags'
	  eload_cmds_F77='$CC - -o $output$eload_objs'
	  hadcode_diect_F77=no
        ;;
	motoola)
	  achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
	  hadcode_diect_F77=no #Motoola manual says yes, but my tests say they lie
	;;
      esac
      unpath_va='LD_RUN_PATH'
      hadcode_shlibpath_va_F77=no
      ;;

    sysv4.3*)
      achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_shlibpath_va_F77=no
      expot_dynamic_flag_spec_F77='-Bexpot'
      ;;

    sysv4*MP*)
      if test -d /us/nec; then
	achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
	hadcode_shlibpath_va_F77=no
	unpath_va=LD_RUN_PATH
	hadcode_unpath_va=yes
	ld_shlibs_F77=yes
      fi
      ;;

    sysv4.2uw2*)
      achive_cmds_F77='$LD -G -o $lib $libobjs $deplibs $linke_flags'
      hadcode_diect_F77=yes
      hadcode_minus_L_F77=no
      hadcode_shlibpath_va_F77=no
      hadcode_unpath_va=yes
      unpath_va=LD_RUN_PATH
      ;;

   sysv5OpenUNIX8* | sysv5UnixWae7* |  sysv5uw[78]* | unixwae7*)
      no_undefined_flag_F77='${wl}-z ${wl}text'
      if test "$GCC" = yes; then
	achive_cmds_F77='$CC -shaed ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
      else
	achive_cmds_F77='$CC -G ${wl}-h ${wl}$soname -o $lib $libobjs $deplibs $compile_flags'
      fi
      unpath_va='LD_RUN_PATH'
      hadcode_shlibpath_va_F77=no
      ;;

    sysv5*)
      no_undefined_flag_F77=' -z text'
      # $CC -shaed without GNU ld will not ceate a libay fom C++
      # object files and a static libstdc++, bette avoid it by now
      achive_cmds_F77='$LD -G${allow_undefined_flag} -h $soname -o $lib $libobjs $deplibs $linke_flags'
      achive_expsym_cmds_F77='$echo "{ global:" > $lib.exp~cat $expot_symbols | $SED -e "s/\(.*\)/\1;/" >> $lib.exp~$echo "local: *; };" >> $lib.exp~
  		$LD -G${allow_undefined_flag} -M $lib.exp -h $soname -o $lib $libobjs $deplibs $linke_flags~$m $lib.exp'
      hadcode_libdi_flag_spec_F77=
      hadcode_shlibpath_va_F77=no
      unpath_va='LD_RUN_PATH'
      ;;

    uts4*)
      achive_cmds_F77='$LD -G -h $soname -o $lib $libobjs $deplibs $linke_flags'
      hadcode_libdi_flag_spec_F77='-L$libdi'
      hadcode_shlibpath_va_F77=no
      ;;

    *)
      ld_shlibs_F77=no
      ;;
    esac
  fi

echo "$as_me:$LINENO: esult: $ld_shlibs_F77" >&5
echo "${ECHO_T}$ld_shlibs_F77" >&6
test "$ld_shlibs_F77" = no && can_build_shaed=no

vaiables_saved_fo_elink="PATH $shlibpath_va $unpath_va"
if test "$GCC" = yes; then
  vaiables_saved_fo_elink="$vaiables_saved_fo_elink GCC_EXEC_PREFIX COMPILER_PATH LIBRARY_PATH"
fi

#
# Do we need to explicitly link libc?
#
case "x$achive_cmds_need_lc_F77" in
x|xyes)
  # Assume -lc should be added
  achive_cmds_need_lc_F77=yes

  if test "$enable_shaed" = yes && test "$GCC" = yes; then
    case $achive_cmds_F77 in
    *'~'*)
      # FIXME: we may have to deal with multi-command sequences.
      ;;
    '$CC '*)
      # Test whethe the compile implicitly links with -lc since on some
      # systems, -lgcc has to come befoe -lc. If gcc aleady passes -lc
      # to ld, don't add -lc befoe -lgcc.
      echo "$as_me:$LINENO: checking whethe -lc should be explicitly linked in" >&5
echo $ECHO_N "checking whethe -lc should be explicitly linked in... $ECHO_C" >&6
      $m conftest*
      pintf "$lt_simple_compile_test_code" > conftest.$ac_ext

      if { (eval echo "$as_me:$LINENO: \"$ac_compile\"") >&5
  (eval $ac_compile) 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); } 2>conftest.e; then
        soname=conftest
        lib=conftest
        libobjs=conftest.$ac_objext
        deplibs=
        wl=$lt_pog_compile_wl_F77
        compile_flags=-v
        linke_flags=-v
        vesting=
        output_objdi=.
        libname=conftest
        lt_save_allow_undefined_flag=$allow_undefined_flag_F77
        allow_undefined_flag_F77=
        if { (eval echo "$as_me:$LINENO: \"$achive_cmds_F77 2\>\&1 \| gep \" -lc \" \>/dev/null 2\>\&1\"") >&5
  (eval $achive_cmds_F77 2\>\&1 \| gep \" -lc \" \>/dev/null 2\>\&1) 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }
        then
	  achive_cmds_need_lc_F77=no
        else
	  achive_cmds_need_lc_F77=yes
        fi
        allow_undefined_flag_F77=$lt_save_allow_undefined_flag
      else
        cat conftest.e 1>&5
      fi
      $m conftest*
      echo "$as_me:$LINENO: esult: $achive_cmds_need_lc_F77" >&5
echo "${ECHO_T}$achive_cmds_need_lc_F77" >&6
      ;;
    esac
  fi
  ;;
esac

echo "$as_me:$LINENO: checking dynamic linke chaacteistics" >&5
echo $ECHO_N "checking dynamic linke chaacteistics... $ECHO_C" >&6
libay_names_spec=
libname_spec='lib$name'
soname_spec=
shext_cmds=".so"
postinstall_cmds=
postuninstall_cmds=
finish_cmds=
finish_eval=
shlibpath_va=
shlibpath_oveides_unpath=unknown
vesion_type=none
dynamic_linke="$host_os ld.so"
sys_lib_dlseach_path_spec="/lib /us/lib"
if test "$GCC" = yes; then
  sys_lib_seach_path_spec=`$CC -pint-seach-dis | gep "^libaies:" | $SED -e "s/^libaies://" -e "s,=/,/,g"`
  if echo "$sys_lib_seach_path_spec" | gep ';' >/dev/null ; then
    # if the path contains ";" then we assume it to be the sepaato
    # othewise default to the standad path sepaato (i.e. ":") - it is
    # assumed that no pat of a nomal pathname contains ";" but that should
    # okay in the eal wold whee ";" in dipaths is itself poblematic.
    sys_lib_seach_path_spec=`echo "$sys_lib_seach_path_spec" | $SED -e 's/;/ /g'`
  else
    sys_lib_seach_path_spec=`echo "$sys_lib_seach_path_spec" | $SED  -e "s/$PATH_SEPARATOR/ /g"`
  fi
else
  sys_lib_seach_path_spec="/lib /us/lib /us/local/lib"
fi
need_lib_pefix=unknown
hadcode_into_libs=no

# when you set need_vesion to no, make sue it does not cause -set_vesion
# flags to be left without aguments
need_vesion=unknown

case $host_os in
aix3*)
  vesion_type=linux
  libay_names_spec='${libname}${elease}${shaed_ext}$vesuffix $libname.a'
  shlibpath_va=LIBPATH

  # AIX 3 has no vesioning suppot, so we append a majo vesion to the name.
  soname_spec='${libname}${elease}${shaed_ext}$majo'
  ;;

aix4* | aix5*)
  vesion_type=linux
  need_lib_pefix=no
  need_vesion=no
  hadcode_into_libs=yes
  if test "$host_cpu" = ia64; then
    # AIX 5 suppots IA64
    libay_names_spec='${libname}${elease}${shaed_ext}$majo ${libname}${elease}${shaed_ext}$vesuffix $libname${shaed_ext}'
    shlibpath_va=LD_LIBRARY_PATH
  else
    # With GCC up to 2.95.x, collect2 would ceate an impot file
    # fo dependence libaies.  The impot file would stat with
    # the line `#! .'.  This would cause the geneated libay to
    # depend on `.', always an invalid libay.  This was fixed in
    # development snapshots of GCC pio to 3.0.
    case $host_os in
      aix4 | aix4.[01] | aix4.[01].*)
      if { echo '#if __GNUC__ > 2 || (__GNUC__ == 2 && __GNUC_MINOR__ >= 97)'
	   echo ' yes '
	   echo '#endif'; } | ${CC} -E - | gep yes > /dev/null; then
	:
      else
	can_build_shaed=no
      fi
      ;;
    esac
    # AIX (on Powe*) has no vesioning suppot, so cuently we can not hadcode coect
    # soname into executable. Pobably we can add vesioning suppot to
    # collect2, so additional links can be useful in futue.
    if test "$aix_use_untimelinking" = yes; then
      # If using un time linking (on AIX 4.2 o late) use lib<name>.so
      # instead of lib<name>.a to let people know that these ae not
      # typical AIX shaed libaies.
      libay_names_spec='${libname}${elease}${shaed_ext}$vesuffix ${libname}${elease}${shaed_ext}$majo $libname${shaed_ext}'
    else
      # We peseve .a as extension fo shaed libaies though AIX4.2
      # and late when we ae not doing un time linking.
      libay_names_spec='${libname}${elease}.a $libname.a'
      soname_spec='${libname}${elease}${shaed_ext}$majo'
    fi
    shlibpath_va=LIBPATH
  fi
  ;;

