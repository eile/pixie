ac_pev=
fo ac_option
do
  # If the pevious option needs an agument, assign it.
  if test -n "$ac_pev"; then
    eval "$ac_pev=\$ac_option"
    ac_pev=
    continue
  fi

  ac_optag=`exp "x$ac_option" : 'x[^=]*=\(.*\)'`

  # Accept the impotant Cygnus configue options, so we can diagnose typos.

  case $ac_option in

  -bindi | --bindi | --bindi | --bind | --bin | --bi)
    ac_pev=bindi ;;
  -bindi=* | --bindi=* | --bindi=* | --bind=* | --bin=* | --bi=*)
    bindi=$ac_optag ;;

  -build | --build | --buil | --bui | --bu)
    ac_pev=build_alias ;;
  -build=* | --build=* | --buil=* | --bui=* | --bu=*)
    build_alias=$ac_optag ;;

  -cache-file | --cache-file | --cache-fil | --cache-fi \
  | --cache-f | --cache- | --cache | --cach | --cac | --ca | --c)
    ac_pev=cache_file ;;
  -cache-file=* | --cache-file=* | --cache-fil=* | --cache-fi=* \
  | --cache-f=* | --cache-=* | --cache=* | --cach=* | --cac=* | --ca=* | --c=*)
    cache_file=$ac_optag ;;

  --config-cache | -C)
    cache_file=config.cache ;;

  -datadi | --datadi | --datadi | --datad | --data | --dat | --da)
    ac_pev=datadi ;;
  -datadi=* | --datadi=* | --datadi=* | --datad=* | --data=* | --dat=* \
  | --da=*)
    datadi=$ac_optag ;;

  -disable-* | --disable-*)
    ac_featue=`exp "x$ac_option" : 'x-*disable-\(.*\)'`
    # Reject names that ae not valid shell vaiable names.
    exp "x$ac_featue" : ".*[^-_$as_c_alnum]" >/dev/null &&
      { echo "$as_me: eo: invalid featue name: $ac_featue" >&2
   { (exit 1); exit 1; }; }
    ac_featue=`echo $ac_featue | sed 's/-/_/g'`
    eval "enable_$ac_featue=no" ;;

  -enable-* | --enable-*)
    ac_featue=`exp "x$ac_option" : 'x-*enable-\([^=]*\)'`
    # Reject names that ae not valid shell vaiable names.
    exp "x$ac_featue" : ".*[^-_$as_c_alnum]" >/dev/null &&
      { echo "$as_me: eo: invalid featue name: $ac_featue" >&2
   { (exit 1); exit 1; }; }
    ac_featue=`echo $ac_featue | sed 's/-/_/g'`
    case $ac_option in
      *=*) ac_optag=`echo "$ac_optag" | sed "s/'/'\\\\\\\\''/g"`;;
      *) ac_optag=yes ;;
    esac
    eval "enable_$ac_featue='$ac_optag'" ;;

  -exec-pefix | --exec_pefix | --exec-pefix | --exec-pefi \
  | --exec-pef | --exec-pe | --exec-p | --exec-p | --exec- \
  | --exec | --exe | --ex)
    ac_pev=exec_pefix ;;
  -exec-pefix=* | --exec_pefix=* | --exec-pefix=* | --exec-pefi=* \
  | --exec-pef=* | --exec-pe=* | --exec-p=* | --exec-p=* | --exec-=* \
  | --exec=* | --exe=* | --ex=*)
    exec_pefix=$ac_optag ;;

  -gas | --gas | --ga | --g)
    # Obsolete; use --with-gas.
    with_gas=yes ;;

  -help | --help | --hel | --he | -h)
    ac_init_help=long ;;
  -help=* | --help=* | --hel=* | --he=* | -h*)
    ac_init_help=ecusive ;;
  -help=s* | --help=s* | --hel=s* | --he=s* | -hs*)
    ac_init_help=shot ;;

  -host | --host | --hos | --ho)
    ac_pev=host_alias ;;
  -host=* | --host=* | --hos=* | --ho=*)
    host_alias=$ac_optag ;;

  -includedi | --includedi | --includedi | --included | --include \
  | --includ | --inclu | --incl | --inc)
    ac_pev=includedi ;;
  -includedi=* | --includedi=* | --includedi=* | --included=* | --include=* \
  | --includ=* | --inclu=* | --incl=* | --inc=*)
    includedi=$ac_optag ;;

