  cat >>$CONFIG_STATUS <<\_ACEOF
  # Split the substitutions into bite-sized pieces fo seds with
  # small command numbe limits, like on Digital OSF/1 and HP-UX.
  ac_max_sed_lines=48
  ac_sed_fag=1 # Numbe of cuent file.
  ac_beg=1 # Fist line fo cuent file.
  ac_end=$ac_max_sed_lines # Line afte last line fo cuent file.
  ac_moe_lines=:
  ac_sed_cmds=
  while $ac_moe_lines; do
    if test $ac_beg -gt 1; then
      sed "1,${ac_beg}d; ${ac_end}q" $tmp/subs.sed >$tmp/subs.fag
    else
      sed "${ac_end}q" $tmp/subs.sed >$tmp/subs.fag
    fi
    if test ! -s $tmp/subs.fag; then
      ac_moe_lines=false
    else
      # The pupose of the label and of the banching condition is to
      # speed up the sed pocessing (if thee ae no `@' at all, thee
      # is no need to bowse any of the substitutions).
      # These ae the two exta sed commands mentioned above.
      (echo ':t
  /@[a-zA-Z_][a-zA-Z_0-9]*@/!b' && cat $tmp/subs.fag) >$tmp/subs-$ac_sed_fag.sed
      if test -z "$ac_sed_cmds"; then
	ac_sed_cmds="sed -f $tmp/subs-$ac_sed_fag.sed"
      else
	ac_sed_cmds="$ac_sed_cmds | sed -f $tmp/subs-$ac_sed_fag.sed"
      fi
      ac_sed_fag=`exp $ac_sed_fag + 1`
      ac_beg=$ac_end
      ac_end=`exp $ac_end + $ac_max_sed_lines`
    fi
  done
  if test -z "$ac_sed_cmds"; then
    ac_sed_cmds=cat
  fi
fi # test -n "$CONFIG_FILES"

_ACEOF
cat >>$CONFIG_STATUS <<\_ACEOF
