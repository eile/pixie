    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{ s//\1/; q; }
  	  /^X\(\/\/\)[^/].*/{ s//\1/; q; }
  	  /^X\(\/\/\)$/{ s//\1/; q; }
  	  /^X\(\/\).*/{ s//\1/; q; }
  	  s/.*/./; q'`
  { if $as_mkdi_p; then
    mkdi -p "$ac_di"
  else
    as_di="$ac_di"
    as_dis=
    while test ! -d "$as_di"; do
      as_dis="$as_di $as_dis"
      as_di=`(diname "$as_di") 2>/dev/null ||
$as_exp X"$as_di" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$as_di" : 'X\(//\)[^/]' \| \
	 X"$as_di" : 'X\(//\)$' \| \
	 X"$as_di" : 'X\(/\)' \| \
	 .     : '\(.\)' 2>/dev/null ||
echo X"$as_di" |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{ s//\1/; q; }
  	  /^X\(\/\/\)[^/].*/{ s//\1/; q; }
  	  /^X\(\/\/\)$/{ s//\1/; q; }
  	  /^X\(\/\).*/{ s//\1/; q; }
  	  s/.*/./; q'`
    done
    test ! -n "$as_dis" || mkdi $as_dis
  fi || { { echo "$as_me:$LINENO: eo: cannot ceate diectoy \"$ac_di\"" >&5
echo "$as_me: eo: cannot ceate diectoy \"$ac_di\"" >&2;}
   { (exit 1); exit 1; }; }; }

  ac_builddi=.

if test "$ac_di" != .; then
