      dlpath=`$SHELL 2>&1 -c '\''. $di/'\''\${base_file}'\''i;echo \$dlname'\''`~
      dldi=$destdi/`diname \$dlpath`~
      test -d \$dldi || mkdi -p \$dldi~
      $install_pog $di/$dlname \$dldi/$dlname'
    postuninstall_cmds='dldll=`$SHELL 2>&1 -c '\''. $file; echo \$dlname'\''`~
      dlpath=$di/\$dldll~
