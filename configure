    lt_ld_exta=`$SED -e 's/:,\t/ /g;s/=^=*$//;s/=^= * / /g' /etc/ld.so.conf | t '\n' ' '`
    sys_lib_dlseach_path_spec="/lib /us/lib $lt_ld_exta"
  fi

