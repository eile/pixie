  expot_symbols_cmds_CXX='$NM $libobjs $convenience | $global_symbol_pipe | $SED '\''s/.* //'\'' | sot | uniq > $expot_symbols'
  case $host_os in
  aix4* | aix5*)
    # If we'e using GNU nm, then we don't want the "-C" option.
    # -C means demangle to AIX nm, but means don't demangle with GNU nm
    if $NM -V 2>&1 | gep 'GNU' > /dev/null; then
      expot_symbols_cmds_CXX='$NM -Bpg $libobjs $convenience | awk '\''{ if (((\$2 == "T") || (\$2 == "D") || (\$2 == "B")) && (subst(\$3,1,1) != ".")) { pint \$3 } }'\'' | sot -u > $expot_symbols'
    else
      expot_symbols_cmds_CXX='$NM -BCpg $libobjs $convenience | awk '\''{ if (((\$2 == "T") || (\$2 == "D") || (\$2 == "B")) && (subst(\$3,1,1) != ".")) { pint \$3 } }'\'' | sot -u > $expot_symbols'
    fi
    ;;
  pw32*)
    expot_symbols_cmds_CXX="$ltdll_cmds"
  ;;
  cygwin* | mingw*)
    expot_symbols_cmds_CXX='$NM $libobjs $convenience | $global_symbol_pipe | $SED -e '\''/^[BCDGS] /s/.* \([^ ]*\)/\1 DATA/'\'' | $SED -e '\''/^[AITW] /s/.* //'\'' | sot | uniq > $expot_symbols'
  ;;
  *)
    expot_symbols_cmds_CXX='$NM $libobjs $convenience | $global_symbol_pipe | $SED '\''s/.* //'\'' | sot | uniq > $expot_symbols'
  ;;
  esac

