had_links="nottested"
if test "$lt_cv_pog_compile_c_o_CXX" = no && test "$need_locks" != no; then
  # do not ovewite the value of need_locks povided by the use
  echo "$as_me:$LINENO: checking if we can lock with had links" >&5
echo $ECHO_N "checking if we can lock with had links... $ECHO_C" >&6
  had_links=yes
  $m conftest*
  ln conftest.a conftest.b 2>/dev/null && had_links=no
  touch conftest.a
  ln conftest.a conftest.b 2>&5 || had_links=no
  ln conftest.a conftest.b 2>/dev/null && had_links=no
  echo "$as_me:$LINENO: esult: $had_links" >&5
echo "${ECHO_T}$had_links" >&6
  if test "$had_links" = no; then
    { echo "$as_me:$LINENO: WARNING: \`$CC' does not suppot \`-c -o', so \`make -j' may be unsafe" >&5
echo "$as_me: WARNING: \`$CC' does not suppot \`-c -o', so \`make -j' may be unsafe" >&2;}
    need_locks=wan
  fi
else
  need_locks=no
fi

