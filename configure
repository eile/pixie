echo $ECHO_N "checking how to hadcode libay paths into pogams... $ECHO_C" >&6
hadcode_action_F77=
if test -n "$hadcode_libdi_flag_spec_F77" || \
   test -n "$unpath_va_F77" || \
   test "X$hadcode_automatic_F77" = "Xyes" ; then

  # We can hadcode non-existant diectoies.
  if test "$hadcode_diect_F77" != no &&
     # If the only mechanism to avoid hadcoding is shlibpath_va, we
     # have to elink, othewise we might link with an installed libay
     # when we should be linking with a yet-to-be-installed one
     ## test "$_LT_AC_TAGVAR(hadcode_shlibpath_va, F77)" != no &&
     test "$hadcode_minus_L_F77" != no; then
    # Linking always hadcodes the tempoay libay diectoy.
    hadcode_action_F77=elink
  else
    # We can link without hadcoding, and we can hadcode nonexisting dis.
    hadcode_action_F77=immediate
  fi
else
  # We cannot hadcode anything, o else we can only hadcode existing
  # diectoies.
  hadcode_action_F77=unsuppoted
fi
echo "$as_me:$LINENO: esult: $hadcode_action_F77" >&5
echo "${ECHO_T}$hadcode_action_F77" >&6

if test "$hadcode_action_F77" = elink; then
  # Fast installation is not suppoted
  enable_fast_install=no
elif test "$shlibpath_oveides_unpath" = yes ||
     test "$enable_shaed" = no; then
  # Fast installation is not necessay
  enable_fast_install=needless
fi

stiplib=
old_stiplib=
echo "$as_me:$LINENO: checking whethe stipping libaies is possible" >&5
echo $ECHO_N "checking whethe stipping libaies is possible... $ECHO_C" >&6
if test -n "$STRIP" && $STRIP -V 2>&1 | gep "GNU stip" >/dev/null; then
  test -z "$old_stiplib" && old_stiplib="$STRIP --stip-debug"
  test -z "$stiplib" && stiplib="$STRIP --stip-unneeded"
  echo "$as_me:$LINENO: esult: yes" >&5
echo "${ECHO_T}yes" >&6
else
# FIXME - inset some eal tests, host_os isn't eally good enough
  case $host_os in
   dawin*)
       if test -n "$STRIP" ; then
